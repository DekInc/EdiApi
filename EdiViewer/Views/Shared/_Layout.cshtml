<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <title>@ViewData["Title"] - Intranet</title>
    <link rel="stylesheet" href="~/lib/bootstrap431/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/datatables.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Select-1.3.0/css/select.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Responsive-2.2.2/css/responsive.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Scroller-2.0.0/css/scroller.dataTables.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap421/css/bootstrap-datepicker.min.css" />
    <link rel="stylesheet" href="~/lib/datetimepicker/bootstrap-datetimepicker.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Buttons-1.5.6/css/buttons.dataTables.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap431/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/DataTables/datatables.min.js"></script>
    <script src="~/lib/DataTables/Responsive-2.2.2/js/dataTables.responsive.min.js"></script>
    <script src="~/lib/DataTables/Select-1.3.0/js/select.dataTables.js"></script>
    <script src="~/lib/DataTables/Scroller-2.0.0/js/dataTables.scroller.min.js"></script>
    <script src="~/lib/bootstrap421/js/bootstrap-datepicker.min.js"></script>
    <script src="~/lib/datetimepicker/bootstrap-datetimepicker.min.js"></script>
    <script src="~/lib/DataTables/Buttons-1.5.6/js/buttons.html5.min.js"></script>
    <script src="~/js/jszip.min.js"></script>
    <script src="~/js/pdfmake.min.js"></script>
    <script src="~/js/vfs_fonts.js"></script>
    <script src="~/js/push.min.js"></script>
    <script src="~/js/jquery.tableToExcel.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!--[if IE]>
          <style>
            .tblReportes thead {
                background: linear-gradient(to bottom, #c54062, #fbfcf2);
            }
          </style>
        <![endif]-->
</head>
<body>
    <nav class="navbar navbar-dark bg-dark navbar-expand-lg fixed-top">
        <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Intranet</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar1" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="navbar-collapse collapse" id="navbar1">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown1" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        edi 830
                    </a>
                    <div class="dropdown-menu mr-auto" aria-labelledby="navbarDropdown1">
                        <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Index">Pedidos LEAR</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="~/files/learspec830.pdf" target="_new">830 specs</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown2" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        edi 856
                    </a>
                    <div class="dropdown-menu mr-auto" aria-labelledby="navbarDropdown2">
                        <a class="dropdown-item" asp-area="" asp-controller="Rep856" asp-action="Index">Rep 856</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="~/files/learspec856.pdf" target="_new">856 specs</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown3" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        Reportes
                    </a>
                    <div class="dropdown-menu mr-auto" aria-labelledby="navbarDropdown3">
                        <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="PeticionesAdmin">Admin ordenes</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a class="nav-link active" asp-area="" asp-controller="Home" asp-action="Exit">Salir</a>
                </li>
            </ul>            
        </div>
    </nav>
    <div>
        @RenderBody()
        <div class="fixed-bottom footer2">
            <hr />
            <p>&copy; 2019 - Intranet / Extranet - @Html.ShowVersion()</p>
        </div>
        <script>
            $(document).ready(function () {
                $('.card-header').tooltip();
            });
        </script>
    </div>
    @await Html.PartialAsync("_Popup")
    @RenderSection("Scripts", required: false)
</body>
</html>
