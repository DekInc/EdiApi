<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Visor EDI</title>
    <link rel="stylesheet" href="~/lib/bootstrap421/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/datatables.min.css" />
    <link rel="stylesheet" href="~/lib/DataTables/Select-1.3.0/css/select.dataTables.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap431/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/DataTables/datatables.min.js"></script>
    <script src="~/lib/DataTables/Select-1.3.0//js/select.dataTables.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</head>
<body>
    <div>
        @RenderBody()
        <div class="fixed-bottom footer2">
            <hr />
            <p>&copy; 2019 - EdiViewer - @Html.ShowVersion()</p>
        </div>
        <script>
            $(document).ready(function () {
                $('.card-header').tooltip();
            });
        </script>
    </div>
    @await Html.PartialAsync("_Popup")
    @RenderSection("Scripts", required: false)
</body>
</html>
