@model Tuple<ComModels.LearPrs830, IEnumerable<ComModels.EdiSegName>>
<div class="card box-shadow">
    <div class="card-header card-cprs cursor" data-toggle="collapse" data-target="#@Model.Item1.HashId" aria-expanded="false" aria-controls="@Model.Item1.HashId">
        PRS - @Model.Item2.Where(Sn => Sn.Segment == "PRS").FirstOrDefault().Eng
    </div>
    <div class="collapse" id="@Model.Item1.HashId">
        <div class="card-body">
            <div class="container">
                <div class="row">
                    <div class="col-sm">
                        <div class="SegmentDataCol">
                            <label>StatusCode:</label>
                            @Model.Item1.StatusCode
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>